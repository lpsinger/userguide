{
    "name": "Alert",
    "namespace": "igwn.alerts.v1_0",
    "type": "record",
    "doc": "Alert schema v1.0.",
    "fields": [
        {"name": "author", "type": "string"},
        {"name": "alert_type", "type": "igwn.alerts.v1_0.AlertType", "doc": "The type of alert; the possible values are EARLY_WARNING, PRELIMINARY, INITIAL, UPDATE, RETRACTION."},
        {"name": "time_created", "type": "string", "doc": "The time the superevent was created in ISO 8601 format."},
        {"name": "superevent_id", "type": "string", "doc": "The GraceDB superevent ID."},
        {"name": "event", "type": ["null", "igwn.alerts.v1_0.EventInfo"], "doc": "Information about the event, if any."},
        {"name": "external_coinc", "type": ["null", "igwn.alerts.v1_0.ExternalCoincInfo"], "doc": "Information about the coincidence with an non-GW event, if any."},
        {"name": "urls", "type": {"type": "map", "values": "string", "default": {}}, "doc": "URLs relevant to the event, if any."}
    ]
}
